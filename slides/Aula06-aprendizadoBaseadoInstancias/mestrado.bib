@ARTICLE{Welty01,
  author = {Christopher Welty and Nicola Guarino},
  title = {Supporting ontological analysis of taxonomic relationships},
  journal = {Data and Knowledge Engineering},
  year = {2001},
  volume = {39},
  pages = {51-74},
  number = {1},
  month = {October},
  abstract = {Taxonomies are an important part of conceptual modelling. They provide
	substancial strucutural information, and are typically the key elements
	in integration efforts, however there has been little guidance as
	to what makes a proper taxonomy. We had adopted several notions from
	the philosophical practice of formal ontology, and adapted them for
	use in information systems. These tools, identity, essence, unity,
	and dependence, provide a solid logical framework within which the
	properties that form a taxonomy can be analyzed. This analysis helps
	make intended meaning more explicit, improving human understanding
	and reducing the cost of integration.},
  dvvvoi = {http://www.sciencedirect.com/science/article/B6TYX-445GF1R-4/2/16058e258e41c6f2e74f048e80eaf707},
}

@INCOLLECTION{Wielinga94,
  author = {B. J. Wielinga and A. Th. Schreiber},
  title = {Conceptual modelling of large reusable knowledge bases},
  booktitle = {Management and Processing of Complex Data Structures},
  year = {1994},
  editor = {K. von Luck and H. Marburger},
  volume = {777},
  series = {Lecture Notes in Computer Science},
  pages = {181-200},
  abstract = {Large amounts of knowledge are available in many knowledge bases for
	a variety of reapplications. This knowledge is however usually application
	specific, and thus not reusable. This paper discusses the problem
	of making knowledge shareable over applications and reusing it. Three
	principles are formulated that can form a basis for a methodology
	for designing a shareable knowledge bases. The separation of domain
	and control knowledge, the explication of meta-models of the domain
	knowledge (ontologies), and the distinction between ontologies tha
	submit to different classes of assumptions commitments are described
	as ways of achieving shareable and reusable knowledge bases.},
  optaddress = {Berlin, Germany},
